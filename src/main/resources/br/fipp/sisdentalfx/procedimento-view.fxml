<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.fipp.sisdentalfx.ProcedimentoViewController">
   <padding>
      <Insets bottom="5.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <Label text="ID">
      <VBox.margin>
         <Insets top="5.0" />
      </VBox.margin>
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <TextField fx:id="tfId" editable="false">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <Label layoutX="30.0" layoutY="50.0" text="Descrição">
      <VBox.margin>
         <Insets top="5.0" />
      </VBox.margin>
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <TextField fx:id="tfDescricao" layoutX="30.0" layoutY="71.0">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <Label layoutX="30.0" layoutY="106.0" text="Tempo">
      <VBox.margin>
         <Insets top="5.0" />
      </VBox.margin>
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <TextField fx:id="tfTempo" layoutX="30.0" layoutY="127.0">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <Label layoutX="30.0" layoutY="162.0" text="Valor">
      <VBox.margin>
         <Insets top="5.0" />
      </VBox.margin>
      <font>
         <Font size="13.0" />
      </font>
   </Label>
   <TextField fx:id="tfValor" layoutX="30.0" layoutY="183.0">
      <font>
         <Font size="14.0" />
      </font>
   </TextField>
   <HBox alignment="CENTER" prefHeight="66.0" prefWidth="560.0" spacing="40.0">
      <Button fx:id="btnConfirmar" minHeight="30.0" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: lightblue;" text="Confirmar" />
      <Button fx:id="btnCancelar" minHeight="30.0" mnemonicParsing="false" prefWidth="90.0" style="-fx-background-color: salmon;" text="Cancelar" />
   </HBox>
</VBox>
